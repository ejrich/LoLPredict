    "version": "0.2.0",
    "configurations": [
        {
            "name": "Database",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/Database/LoLPredict.Database/bin/Debug/netcoreapp2.2/LoLPredict.Database.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/Database/LoLPredict.Database",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Attach to .NET Functions",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:azureFunctions.pickProcess}"
        }
    ]
}